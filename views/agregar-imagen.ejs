  <div class="container-contacto">
    
    
    <form method="POST" action="/imagen">
      <h2>Agregar imagen</h2>
      <label for="url">url:</label>
      <input type="text" id="url" name="url" required>
      <label for="producto">inserte id:</label>
      <input type="text" id="producto_id" name="producto_id" required>
      <label for="destacado">destacado:</label>
      <input type="text" id="destacado" name="destacado" required>
      <button class="ver-todo" type="submit">Agregar</button>
    </form>
  </div>

    <footer class="footer-section">
